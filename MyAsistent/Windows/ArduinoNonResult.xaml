<Page x:Class="MyAsistent.Windows.ArduinoNonResult"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MyAsistent.Windows"
        mc:Ignorable="d"
        Title="ArduinoNonResult" Height="250" Width="230">
    <Grid>
        <GroupBox Header="Модулі">
            <Grid >
                <Grid.RowDefinitions>
                    <RowDefinition Height="173*"/>
                    <RowDefinition Height="48*"/>
                    <RowDefinition Height="48*"/>
                </Grid.RowDefinitions>
                <ListBox Name="ArduinoCommand" Grid.Row="0"/>
                <ComboBox Name="ArduinoName" Margin="2 2 2 2" Grid.Row="1"/>
                <Button Click="Button_Click" Name="Send" Content="Ok" Grid.Row="2" Margin="2 2 2 2" />
            </Grid>
        </GroupBox>

    </Grid>
</Page>
